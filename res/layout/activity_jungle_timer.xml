<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_vertical"
    android:background="@drawable/bg"
    android:fitsSystemWindows="true"
    android:id="@+id/container"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:visibility="visible"
    tools:context="com.fourfoureight.lolhelper.JungleTimer" >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="239dp" >
    </RelativeLayout>

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:layout_alignLeft="@+id/ImageButton3"
        android:layout_alignRight="@+id/relativeLayout1"
        android:adjustViewBounds="true"
        android:background="#7A1F00"
        android:contentDescription="@id/imageButton1"
        android:onClick="RedGolem"
        android:scaleType="fitCenter"
        android:src="@drawable/golem" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:layout_alignLeft="@+id/relativeLayout1"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/tableRow2"
        android:background="#7A1F00"
        android:contentDescription="@id/imageButton2"
        android:onClick="RedLizard"
        android:scaleType="fitCenter"
        android:src="@drawable/lizard" />

    <ImageButton
        android:id="@+id/ImageButton3"
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:layout_alignLeft="@+id/ImageButton5"
        android:layout_alignRight="@+id/relativeLayout1"
        android:layout_below="@id/imageButton1"
        android:adjustViewBounds="true"
        android:background="#001F3D"
        android:contentDescription="@id/ImageButton3"
        android:onClick="BlueGolem"
        android:scaleType="fitCenter"
        android:src="@drawable/golem" />

    <ImageButton
        android:id="@+id/ImageButton4"
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:layout_alignLeft="@+id/relativeLayout1"
        android:layout_alignRight="@+id/tableRow4"
        android:layout_below="@id/imageButton2"
        android:background="#001F3D"
        android:contentDescription="@id/ImageButton4"
        android:onClick="BlueLizard"
        android:scaleType="fitCenter"
        android:src="@drawable/lizard" />

    <ImageButton
        android:id="@+id/ImageButton5"
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:layout_alignRight="@+id/relativeLayout1"
        android:layout_below="@+id/ImageButton3"
        android:adjustViewBounds="true"
        android:background="#3C3C3C"
        android:contentDescription="@id/ImageButton5"
        android:onClick="Dragon"
        android:scaleType="fitCenter"
        android:src="@drawable/dragon" />

    <ImageButton
        android:id="@+id/ImageButton6"
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:layout_below="@id/ImageButton4"
        android:layout_toRightOf="@+id/relativeLayout1"
        android:background="#3C3C3C"
        android:contentDescription="@id/ImageButton6"
        android:onClick="Baron"
        android:scaleType="fitCenter"
        android:src="@drawable/baron" />

    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/relativeLayout1"
        android:layout_alignTop="@+id/ImageButton3"
        android:background="#262727"
        android:gravity="center" >

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Blue Golem"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#808182"
            android:textSize="10dp" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/ImageButton4"
        android:layout_toRightOf="@+id/relativeLayout1"
        android:background="#262727"
        android:gravity="center" >

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Blue Lizard"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#808182"
            android:textSize="10dp" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignRight="@id/ImageButton5"
        android:layout_alignTop="@id/ImageButton5"
        android:background="#262727"
        android:gravity="center" >

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Dragon"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#808182"
            android:textSize="10dp" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/ImageButton6"
        android:layout_toRightOf="@+id/relativeLayout1"
        android:background="#262727"
        android:gravity="center" >

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Baron"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#808182"
            android:textSize="10dp" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/relativeLayout1"
        android:background="#262727"
        android:gravity="center" >

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:contentDescription="Red Golem"
            android:text="Red Golem"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#808182"
            android:textSize="10dp" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/tableRow7"
        android:background="#262727"
        android:gravity="center" >

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Red Lizard"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#808182"
            android:textSize="10dp" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow7"
        android:layout_width=".01dp"
        android:layout_height="375dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="#262727"
        android:visibility="invisible" >

    </TableRow>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButton1"
        android:layout_below="@+id/tableRow1"
        android:text="RG"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tableRow4"
        android:layout_below="@+id/tableRow2"
        android:text="RL"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tableRow4"
        android:layout_below="@+id/tableRow4"
        android:text="BL"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ImageButton6"
        android:layout_below="@+id/tableRow6"
        android:text="BA"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ImageButton5"
        android:layout_below="@+id/tableRow5"
        android:text="DR"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ImageButton3"
        android:layout_below="@+id/tableRow3"
        android:text="BG"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#ffffff" />
    
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:gravity="bottom"
        android:id="@+id/container2">
    </LinearLayout>  

</RelativeLayout>

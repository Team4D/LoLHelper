<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/content"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
	<LinearLayout
	    android:id="@+id/container"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:paddingLeft="25dp"
	    android:orientation="vertical" >
	    
	    <LinearLayout
		    android:id="@+id/container0"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal"
		    android:layout_marginBottom="10dp" >
		       <TextView
		           android:id="@+id/BuildHead"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:textColor="#FFFFFF"
		           android:textStyle="bold"
		           android:textSize="25dp"
		           android:text="Build Name" />
		</LinearLayout>
		
		<LinearLayout
		    android:id="@+id/container1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal"
		    android:layout_marginBottom="5dp" >
		       <TextView
		           android:id="@+id/StartHead"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:textColor="#FFFFFF"
		           android:textStyle="bold"
		           android:text="Starting Items" />
		</LinearLayout>
		
		<LinearLayout
		    android:id="@+id/container2"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal"
		    android:layout_marginBottom="10dp" >
		       <ImageView
		           android:id="@+id/imageViewStart1"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:src="@drawable/total_transparent" />
		       <ImageView
		           android:id="@+id/imageViewStart2"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:layout_marginLeft="5dp"
		           android:src="@drawable/total_transparent" />
		       <ImageView
		           android:id="@+id/imageViewStart3"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:layout_marginLeft="5dp"
		           android:src="@drawable/total_transparent" />
		       <ImageView
		           android:id="@+id/imageViewStart4"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:layout_marginLeft="5dp"
		           android:src="@drawable/total_transparent" />
		</LinearLayout>
		
		<LinearLayout
		    android:id="@+id/container3"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal"
		    android:layout_marginBottom="5dp" >
		       <TextView
		           android:id="@+id/RushHead"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:textColor="#FFFFFF"
		           android:textStyle="bold"
		           android:text="Rush" />
		</LinearLayout>
		
		<LinearLayout
		    android:id="@+id/container4"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal"
		    android:layout_marginBottom="10dp" >
		       <ImageView
		           android:id="@+id/imageViewRush1"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:src="@drawable/total_transparent" />
		       <ImageView
		           android:id="@+id/imageViewRush2"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:layout_marginLeft="5dp"
		           android:src="@drawable/total_transparent" />
		       <ImageView
		           android:id="@+id/imageViewRush3"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:layout_marginLeft="5dp"
		           android:src="@drawable/total_transparent" />
		              <ImageView
		           android:id="@+id/imageViewRush4"
		           android:layout_width="wrap_content"
		           android:layout_height="wrap_content"
		           android:layout_marginLeft="5dp"
		           android:src="@drawable/total_transparent" />
		</LinearLayout>
		       
		<LinearLayout
			    android:id="@+id/container5"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:orientation="horizontal"
			    android:layout_marginBottom="5dp" >
			       <TextView
			           android:id="@+id/AsNeededHead"
			           android:layout_width="wrap_content"
			           android:layout_height="wrap_content"
			           android:textColor="#FFFFFF"
			           android:textStyle="bold"
			           android:text="As Needed" />
		</LinearLayout>
		<LinearLayout
			    android:id="@+id/container6"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:orientation="horizontal" >
			       <ImageView
			           android:id="@+id/imageViewAsNeeded1"
			           android:layout_width="wrap_content"
			           android:layout_height="wrap_content"
			           android:src="@drawable/total_transparent" />
			       <ImageView
			           android:id="@+id/imageViewAsNeeded2"
			           android:layout_width="wrap_content"
			           android:layout_height="wrap_content"
			           android:layout_marginLeft="5dp"
			           android:src="@drawable/total_transparent" />
			       <ImageView
			           android:id="@+id/imageViewAsNeeded3"
			           android:layout_width="wrap_content"
			           android:layout_height="wrap_content"
			           android:layout_marginLeft="5dp"
			           android:src="@drawable/total_transparent" />
			       <ImageView
			           android:id="@+id/imageViewAsNeeded4"
			           android:layout_width="wrap_content"
			           android:layout_height="wrap_content"
			           android:layout_marginLeft="5dp"
			           android:src="@drawable/total_transparent" />
			       <ImageView
			           android:id="@+id/imageViewAsNeeded5"
			           android:layout_width="wrap_content"
			           android:layout_height="wrap_content"
			           android:layout_marginLeft="5dp"
			           android:src="@drawable/total_transparent" />
		</LinearLayout>
<!-- 		<Button 
		    		android:id="@+id/additionalInfo"
		          	android:layout_width="wrap_content"
		           	android:layout_height="wrap_content"
		           	android:layout_marginTop="30dp"
		   		 	android:text="Additional Information"/> -->
	</LinearLayout>
</ScrollView>